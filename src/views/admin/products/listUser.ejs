<!DOCTYPE html>
<html lang="en">
<%-include("../partials/head") %>
<body>
 <%-include("../partials/header") %> 
 <main class="container min-vh-100 pt-5">
    <table class="table table-striped table-hover">
        <thead>
          <tr>
            <th scope="col">id</th>
             <th scope="col">Usuario</th>
            <!--<th scope="col">Apellido</th>
            <th scope="col">Categor√≠a</th>
            <-- <th scope="col">Emprendimiento</th> -->
            <!-- <th scope="col">Telefono</th>
            <th scope="col">Email</th> -->
            <th scope="col">Rol De Usuario</th>
            <th scope="col"></th>
          </tr>
        </thead>
        <tbody>
            <% usuarios.forEach(user => { %>
                <tr>
                  <th scope="row"><%- user.id %></th>
                  <td><%- user.nombre %></td>
                  <!-- <td><%- user.apellido %></td>
                  <td><%- user.fecha %></td>
                  <td><%- user.telefono %></td>
                  <td><%- user.email %></td> --> 
                  <!-- <td><%- user.avatar %></td> -->
                  <td><%- user.roluser_id %></td>
                  <td>
                    <a href="/administrador/usuarios/editar/<%-user.id%>" class="btn btn-success">
                      <i class="fa-solid fa-pen-to-square"></i>
                    </a>

                    <form action="/administrador/usuarios/<%= user.id %>?_method=DELETE" method="POST">
                      <button class="btn btn-danger" type="submit" >
                        <i class="fa-solid fa-trash-can"></i>
                      </button>
                    </form>
                    
                  </td>
                </tr>
              <% }) %>  
            </tbody>
          </table>
      </main>

 <%-include("../partials/footer") %> 
    <!--Bootstrap-->
<%-include("../partials/script") %> 
</body>
</html>